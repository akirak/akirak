{
  "name": "akirak",
  "version": "1.0.0",
  "description": "Generate my GitHub readme",
  "author": "Akira Komamura",
  "keywords": [],
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "build:readme": "rm README.md && bun run src/readme.ts",
    "build:star-history": "bun run src/star-history.ts",
    "build:all": "bun run build:star-history && bun run build:readme"
  },
  "dependencies": {
    "bun-types": "^1.1.36",
    "hast-util-to-html": "^9.0.3",
    "hastscript": "^9.0.0",
    "quickchart-js": "^3.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
    "@types/hast": "^3.0.4",
    "eslint": "^9.15.0",
    "typescript": "^5.6.3"
  }
}
